<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.ColorAdjust?>
<?import javafx.scene.effect.Light.Distant?>
<?import javafx.scene.effect.Lighting?>
<?import javafx.scene.effect.Shadow?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.paint.Color?>
<?import javafx.scene.text.Font?>

<VBox fx:id="tsk" alignment="CENTER" prefHeight="367.0" prefWidth="598.0" spacing="20.0" style="-fx-border-color: black;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.bk10.todolist.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <ListView fx:id="taskList" prefHeight="200.0" prefWidth="200.0">
      <VBox.margin>
         <Insets />
      </VBox.margin></ListView>
   <ButtonBar maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="40.0" prefWidth="560.0">
     <buttons>
         <TextField fx:id="inputTask" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" onAction="#inputTask" prefHeight="25.0" prefWidth="515.0" promptText="Enter a Task!" translateX="-10.0">
            <effect>
               <Lighting diffuseConstant="1.18" specularConstant="2.0" specularExponent="27.36" surfaceScale="0.0">
                  <bumpInput>
                     <Shadow />
                  </bumpInput>
                  <light>
                     <Light.Distant>
                        <color>
                           <Color red="0.9890432357788086" green="0.6712962985038757" blue="1.0" />
                        </color>
                     </Light.Distant>
                  </light>
               </Lighting>
            </effect>
         </TextField>
         <DatePicker cache="true" maxHeight="-Infinity" maxWidth="212.0" minHeight="25.0" minWidth="25.0" nodeOrientation="LEFT_TO_RIGHT" onAction="#date" prefHeight="25.0" prefWidth="25.0" promptText="DEADLINE" showWeekNumbers="true" translateX="-10.0">
            <effect>
               <Lighting surfaceScale="0.0">
                  <bumpInput>
                     <Lighting bumpInput="$null">
                        <light>
                           <Light.Distant />
                        </light>
                     </Lighting>
                  </bumpInput>
                  <light>
                     <Light.Distant>
                        <color>
                           <Color red="0.803600013256073" green="0.9800000190734863" blue="0.9211999773979187" />
                        </color>
                     </Light.Distant>
                  </light>
               </Lighting>
            </effect>
         </DatePicker>
     </buttons>
   </ButtonBar>
   <ButtonBar focusTraversable="false" prefHeight="25.0" prefWidth="485.0" scaleShape="false">
     <buttons>
          <Button defaultButton="true" onAction="#addTask" text="Add Task" translateX="-90.0">
            <effect>
               <Lighting bumpInput="$null" diffuseConstant="1.34" specularConstant="0.0" specularExponent="19.08" surfaceScale="0.0">
                  <light>
                     <Light.Distant>
                        <color>
                           <Color red="0.5045714378356934" green="1.0" blue="0.3199999928474426" />
                        </color>
                     </Light.Distant>
                  </light>
               </Lighting>
            </effect>
            <font>
               <Font name="System Bold" size="12.0" />
            </font></Button>
         <Button defaultButton="true" mnemonicParsing="false" onAction="#markAsCompleted" text="Mark As Completed" translateX="-80.0" />
         <Button defaultButton="true" mnemonicParsing="false" onAction="#deleteTask" text="Delete Task" translateX="-70.0">
            <effect>
               <Lighting diffuseConstant="2.0" surfaceScale="0.0">
                  <bumpInput>
                     <Lighting bumpInput="$null">
                        <light>
                           <Light.Distant />
                        </light>
                     </Lighting>
                  </bumpInput>
                  <light>
                     <Light.Distant>
                        <color>
                           <Color red="1.0" green="0.08757716417312622" blue="0.004629629664123058" />
                        </color>
                     </Light.Distant>
                  </light>
               </Lighting>
            </effect>
            <font>
               <Font name="System Bold" size="12.0" />
            </font></Button>
     </buttons>
   </ButtonBar>
    <Label fx:id="welcomeText" />
   <effect>
      <ColorAdjust />
   </effect>
</VBox>
